{
  "name": "YouTube\u2122 SubSort",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension re-sorts the youtube subscription page based on a scoring algorithm instead of by date uploaded.",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },
  
  "background": {
    "scripts": [ "background.js" ]
  },
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "script.js" ],
    "matches": [ "*://*.youtube.com/feed/subscriptions" ],
    "run_at": "document_start"
  } ],
  "options_page": "popup.html",
  "page_action": {
    "default_icon": "icon38.png",
    "default_popup": "popup.html",
    "default_title": "YouTube Re-Sort"
  },
  "permissions": [
    "http://www.youtube.com/feed/subscriptions",
    "storage"
  ]
}
