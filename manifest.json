{
  "manifest_version": 2,

  "name": "YouTube SubSort",
  "description": "This extension re-sorts the youtube subscription page based a scoring algorithm instead of by date uploaded.",
  "version": "1.0",

  "permissions": [
    "http://www.youtube.com/feed/subscriptions",
    "storage"
  ],
  
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "script.js" ],
    "matches": [ "http://www.youtube.com/feed/subscriptions" ]
  } ],
  "page_action": {
    "default_icon": {
      "19": "icon.png"
    }
    "default_popup": "popup.html",
    "default_title": "YouTube Re-Sort",
  },
  "options_page": "popup.html"
}
